{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "abraham/twitteroauth": "^0.7.4",
        "albertcht/invisible-recaptcha": "^1.7",
        "barryvdh/laravel-ide-helper": "^2.4",
        "brotzka/laravel-dotenv-editor": "^2.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "greggilbert/recaptcha": "^2.2",
        "illuminate/contracts": "^5.5",
        "illuminate/http": "^5.5",
        "illuminate/support": "^5.5",
        "laracasts/flash": "^3.0",
        "laracasts/generators": "^1.1",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "laravelcollective/remote": "^5.5",
        "league/glide-laravel": "^1.0",
        "mews/purifier": "^2.0",
        "plank/laravel-mediable": "^2.5",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4",
        "shawnsandy/extras": "dev-master",
        "shawnsandy/pagekit": "dev-master",
        "spatie/laravel-collection-macros": "^3.0",
        "thujohn/twitter": "^2.2",
        "zondicons/blade-bridge": "^0.1.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
